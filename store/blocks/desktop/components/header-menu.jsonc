{
    "sticky-layout#desktop": {
        "props": {
            "blockClass": "sticky-header"
        },
        "children": [
            "flex-layout.row#desktop",
            //"flex-layout.row#desktop__navbar",
            "flex-layout.row#desktop__categories"
        ]
    },
    "flex-layout.row#desktop": {
        "props": {
            "horizontalAlign": "center",
            "verticalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "fullWidth": true,
            "colJustify":"around",
            "blockClass":[
                "row__desktop"
            ]
        },
        "children": [
            "flex-layout.col#logo-desktop",
            "search-bar",
            "flex-layout.col#navbar",
            //"flex-layout.col#spacer",
            //"locale-switcher",
            "login",
            "minicart.v2"
        ]
    },
    "flex-layout.col#logo-desktop": {
        "props": {
            "verticalAlign": "middle"
        },
        "children": [
            "logo"
        ]
    },
    "flex-layout.col#navbar":{
        "props":{
            "fullWidth": true,
            "rowGap":"5",
            "blockClass":[
                "col__navbar"
            ]
        },
        "children":[
            "flex-layout.row#desktop__navbar"
        ]
    },
    "flex-layout.row#desktop__navbar":{
        "props": {
            "horizontalAlign": "center",
            "fullWidth": true,
            "blockClass":[
                "desktop__navbar"
            ]
        },
        "children": [
            "rich-text#tendencia",
            "rich-text#coleccion",
            "rich-text#ofertas",
            "rich-text#cyber-monday",
            "rich-text#never"
        ]
    },
    "flex-layout.row#desktop__categories":{
        "props": {
            "horizontalAlign": "center"
        },
        "children":[
