{
    "sticky-layout#desktop": {
        "title": " desktop sticky layout header",
        "props": {
            "blockClass": "sticky-header-desktop"
        },
        "children": [
            "flex-layout.row#desktop",
            "flex-layout.row#global__header__categories"
        ]
    },
    "flex-layout.row#desktop": {
        "title": " desktop row navbar flex-layout",
        "props": {
            "horizontalAlign": "center",
            "verticalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "fullWidth": true,
            "colJustify":"around",
            "blockClass":[
                "row__desktop"
            ]
        },
        "children": [
            "flex-layout.col#desktop__menu--holder",
            "flex-layout.col#desktop__logo",
            "search-bar",
            "flex-layout.col#desktop__navbar--col",
            "minicart.v2",
            "login"
        ]
    },
    "flex-layout.col#desktop__menu--holder": {
        "title": " desktop col drawer flex-layout",
        "props": {
            "verticalAlign": "middle",
            "horizontalAlign": "center",
            "blockClass":[
                "desktop__menu--holder"
            ]
        },
        "children": [
            "vtex.menu@2.x:menu#websites"
        ]
    },
    "flex-layout.col#desktop__logo": {
        "title": " desktop col logo flex-layout",
        "props": {
            "verticalAlign": "middle",
            "horizontalAlign": "center"
        },
        "children": [
            "logo"
        ]
    },
    "flex-layout.col#desktop__navbar--col":{
        "title":" desktop col navbar ",
        "props":{
            "fullWidth": true,
            "width":"grow",
            "horizontalAlign":"center",
            "verticalAlign":"middle",
            "blockClass":[
                "col__navbar"
            ]
        },
        "children":[
            "flex-layout.row#desktop__navbar--row"
        ]
    },
    "flex-layout.row#desktop__navbar--row":{
        "title": " desktop text row navbar ",
        "props": {
            "horizontalAlign": "center",
            "verticalAlign":"middle",
            "fullWidth": true,
            "colSizing":"auto",
            "colGap":5,
            "blockClass":[
                "desktop__navbar"
            ]
        },
        "children": [
            "link#destktop__navbar--tendencia",
            "link#destktop__navbar--coleccion",
            "link#destktop__navbar--ofertas",
            "link#destktop__navbar--cyber-monday",
            "link#destktop__navbar--never"
        ]
    },

    "link#destktop__navbar--tendencia":{
        "props":{
            "label": "Tendencias de ITGlobers",
            "href":"#",
            "displayMode": "anchor",
            "blockClass":[
                "desktop__navbar--links"
            ]
        }
    },
    "link#destktop__navbar--coleccion":{
        "props":{
            "label": "Colecci√≥n del mes",
            "href":"#",
            "displayMode": "anchor",
            "blockClass":[
                "desktop__navbar--links"
            ]
        }
    },
    "link#destktop__navbar--ofertas":{
        "props":{
            "label": "Ofertas del mes",
            "href":"#",
            "displayMode": "anchor",
            "blockClass":[
                "desktop__navbar--links"
            ]
        }
    },
    "link#destktop__navbar--cyber-monday":{
        "props":{
            "label": "Cyber Monday",
            "href":"#",
            "displayMode": "anchor",
            "blockClass":[
                "desktop__navbar--links"
            ]
        }
    },
    "link#destktop__navbar--never":{
        "props":{
            "label": "Never say never",
            "href":"#",
            "displayMode": "anchor",
            "blockClass":[
                "desktop__navbar--links"
            ]
        }
    }
